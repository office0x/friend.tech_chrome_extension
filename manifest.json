{
    "manifest_version": 2,
    "name": "Twitter Friend.Tech Extension",
    "version": "1.0",
    "description": "Friend.tech extension for twitter.com",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
          "matches": ["*://twitter.com/*"],
          "js": ["content.js"],
          "css": ["styles.css"]
        }
      ]
  }
